{
	"name": "flow",
	"version": "0.3.1",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev:wss": "pnpm cross-env PORT=3001 tsx watch src/server/dev.wsServer.ts --tsconfig tsconfig.server.json",
		"dev:next": "next dev",
		"dev": "prisma migrate dev && run-p dev:*",
		"build:next": "prisma generate && pnpm cross-env NODE_ENV=production next build",
		"build:ws": "prisma generate && tsc --project tsconfig.server.json",
		"build": "run-s build:*",
		"start:ws": "pnpm cross-env PORT=3001 node dist/server/prod.wsServer.js",
		"start:next": "prisma migrate deploy && pnpm cross-env PORT=3000 next start",
		"start": "run-p start:*",
		"lint": "next lint"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^1.0.1",
		"@prisma/client": "5.1.1",
		"@radix-ui/react-alert-dialog": "^1.0.4",
		"@radix-ui/react-avatar": "^1.0.3",
		"@radix-ui/react-context-menu": "^2.1.4",
		"@radix-ui/react-dialog": "^1.0.4",
		"@radix-ui/react-dropdown-menu": "^2.0.5",
		"@radix-ui/react-popover": "^1.0.6",
		"@radix-ui/react-select": "^1.2.2",
		"@radix-ui/react-slot": "^1.0.2",
		"@radix-ui/react-toast": "^1.1.4",
		"@tanstack/react-query": "^4.32.6",
		"@trpc/client": "^10.37.1",
		"@trpc/next": "^10.37.1",
		"@trpc/react-query": "^10.37.1",
		"@trpc/server": "^10.37.1",
		"autoprefixer": "10.4.15",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"cmdk": "^0.2.0",
		"lucide-react": "^0.268.0",
		"next": "13.4.16",
		"next-auth": "^4.22.1",
		"next-themes": "^0.2.1",
		"postcss": "8.4.27",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"reactflow": "^11.8.1",
		"styled-components": "^6.0.7",
		"superjson": "^1.13.1",
		"tailwind-merge": "^1.14.0",
		"tailwindcss": "3.3.3",
		"tailwindcss-animate": "^1.0.6",
		"throttle-debounce": "^5.0.0",
		"use-debounce": "^9.0.4",
		"ws": "^8.13.0",
		"zod": "^3.22.0",
		"zustand": "^4.4.1",
		"prisma": "^5.1.1",
		"cross-env": "^7.0.3"
	},
	"devDependencies": {
		"@types/node": "20.5.0",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"@types/throttle-debounce": "^5.0.0",
		"@types/ws": "^8.5.5",
		"nodemon": "^3.0.1",
		"npm-run-all": "^4.1.5",
		"ts-node": "^8.10.2",
		"tsx": "^3.12.7",
		"typescript": "5.1.3"
	}
}
