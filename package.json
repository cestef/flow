	"name": "flow-yjs",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev:next": "next dev",
		"dev:yjs": "tsx watch src/server/index.ts --tsconfig tsconfig.server.json",
		"dev": "prisma migrate dev && run-p dev:*",
		"build:next": "prisma generate && pnpm cross-env NODE_ENV=production next build",
		"build:yjs": "prisma generate && tsc --project tsconfig.server.json",
		"build": "run-s build:*",
		"start:yjs": "pnpm cross-env PORT=3001 node dist/server/index.js",
		"start:next": "prisma migrate deploy && pnpm cross-env PORT=3000 next start",
		"start": "run-p start:*",
		"prepare": "husky install",
		"lint": "eslint --ext .ts,.tsx src"
	},
